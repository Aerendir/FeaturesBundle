{
    "name": "serendipity_hq/features-bundle",
    "description": "Manage features and plans in your Symfony app.",
    "type": "symfony-bundle",
    "keywords": [],
    "license": "MIT",
    "authors": [
        {
            "name": "Adamo Aerendir Crespi",
            "email": "hello@aerendir.me"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-intl": "*",
        "doctrine/common": "^2.5",
        "doctrine/doctrine-bundle": "^1.8",
        "doctrine/orm": "^2.5",
        "serendipity_hq/php_array_writer": "dev-dev",
        "serendipity_hq/php_value_objects": "dev-dev",
        "symfony/form": "^3.0|^4.0|^5.0",
        "symfony/framework-bundle": "^3.0|^4.0|^5.0",
        "symfony/options-resolver": "^3.0|^4.0|^5.0",
        "symfony/routing": "^3.0|^4.0|^5.0",
        "symfony/translation": "^3.0|^4.0|^5.0",
        "thecodingmachine/safe": "^0.1|^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^6|^7|^8",
        "rector/rector": "^0.7.41",
        "roave/security-advisories": "dev-master",
        "serendipity_hq/php_text_matrix": "^2.0",
        "symfony/var-dumper": "^3.0|^4.0|^5.0",
        "thecodingmachine/phpstan-safe-rule": "@stable"
    },
    "suggest": {
        "serendipity_hq/php_text_matrix": "Allows tho use of PlainTextDrawer."
    },
    "autoload": {
        "psr-4": {
            "SerendipityHQ\\Bundle\\FeaturesBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SerendipityHQ\\Bundle\\FeaturesBundle\\Tests\\": "tests"
        }
    },
    "config":{
        "sort-packages": true
    },
    "conflict": {
        "nikic/php-parser": "4.6.0"
    }
}
